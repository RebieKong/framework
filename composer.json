{
    "name": "spark-php/framework",
    "description": "An effective,fast,stable Framework for PHP.",
    "keywords": [
        "php",
        "framework",
        "effective",
        "fast"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Panda Wu",
            "email": "itwujunze@gamil.com"
        }
    ],
    "require": {
        "php": "^7.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "codedungeon/phpunit-result-printer": "^0.23.3"
    },
    "autoload": {
        "psr-4": {
            "Spark\\Framework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spark\\Framework\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": [
            "phpunit"
        ]
    }

}
